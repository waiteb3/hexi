const server = require('../src/app').default

const init = async () => {
    await server.start()
    console.log(`Server running at: ${server.info.uri}`)
}

process.on('unhandledRejection', (err) => {
    console.log(err)
    process.exit(1)
});

init().catch((err) => {
    console.error('Failed to start application. Exiting')
    console.error(err)
    process.exit(1)
})
